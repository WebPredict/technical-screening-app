<% provide(:title, 'Sign up') %>
<div class="page-header">
<h1>Sign Up (<%= link_to "Show Membership Plans", plans_path %>)</h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create My Account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>